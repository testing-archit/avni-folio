{
    "version": 2,
    "name": "avni-portfolio-backend",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DATABASE_URL": "@database_url",
        "WEB3FORMS_ACCESS_KEY": "@web3forms_access_key",
        "WEB3FORMS_TO_EMAIL": "@web3forms_to_email",
        "FRONTEND_URL": "@frontend_url"
    }
}